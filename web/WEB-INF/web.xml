<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    
    
     <servlet>
        <servlet-name>Servlet1</servlet-name>
        <servlet-class>com.example.Server.Servlet1</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>Servlet2</servlet-name>
        <servlet-class>com.example.Server.Servlet2</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>Servlet3</servlet-name>
        <servlet-class>com.example.Server.Servlet3</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>Servlet4</servlet-name>
        <servlet-class>com.example.Server.Servlet4</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>GetFav</servlet-name>
        <servlet-class>com.example.Server.GetFavorite</servlet-class>
    </servlet>
    
    
    
    <servlet-mapping>
        <servlet-name>Servlet1</servlet-name>
        <url-pattern>/Servlet.do</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>Servlet2</servlet-name>
        <url-pattern>/Servlet2.do</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>Servlet3</servlet-name>
        <url-pattern>/Servlet3.do</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>Servlet4</servlet-name>
        <url-pattern>/Servlet4.do</url-pattern>
    </servlet-mapping>
    
    
    <servlet-mapping>
        <servlet-name>GetFav</servlet-name>
        <url-pattern>/GetFav.do</url-pattern>
    </servlet-mapping>
    
</web-app>
